[[u'\u4ee5\u4e0b\u54ea\u4e00\u9879\u4e0d\u5c5e\u4e8e\u975e\u8bed\u8a00\u6027\u6c9f\u901a\u7684\u6e20\u9053\uff1a'], [u'\u901a\u8fc7\u673a\u5236\u521b\u65b0\u548c\u653f\u7b56\u6276\u6301\uff0c\u8ba1\u5212\u5229\u75282\u5e74\u65f6\u95f4\u8bd5\u70b9\uff0c\u9009\u7528\uff08\uff09\u4e07\u540d\u7279\u5c97\u519c\u6280\u4eba\u5458\u5230\u57fa\u5c42\u670d\u52a1\uff1b\u529b\u4e89\u901a\u8fc75\u201410\u5e74\u65f6\u95f4\u52aa\u529b\uff0c\u5b9e\u73b0\u6bcf\u4e2a\u4e61\u9547\u533a\u57df\u5185\u62e5\u6709\uff08C\uff09\u540d\u5de6\u53f3\u7279\u5c97\u519c\u6280\u4eba\u5458\u3002'], [u'\u4ee5\u4e0b\u54ea\u5bb6\u5355\u4f4d\u4e0d\u662f\u201d\u6c5f\u82cf\u7701\u66a8\u5357\u4eac\u5e022016\u5c4a\u666e\u901a\u9ad8\u6821\u6bd5\u4e1a\u751f\u516c\u76ca\u4f9b\u9700\u6d3d\u8c08\u4f1a\u201c\u7684\u627f\u529e\u5355\u4f4d?'], ['"'], [u'\u5e94\u5f81\u5165\u4f0d\u7684\u7537\u6027\u516c\u6c11\u8eab\u9ad8\u8981\u6c42\u4e3a\uff08\uff09\u3002'], [u'\u9000\u5f79\u4eba\u5458\u5728\u7ee7\u7eed\u5b9e\u884c\u666e\u901a\u9ad8\u6821\u5e94\u5c4a\u6bd5\u4e1a\u751f\u9000\u5f79\u540e\u6309\u89c4\u5b9a\u4eab\u53d7\u52a0\u5206\u653f\u7b56\u7684\u57fa\u7840\u4e0a\uff0c\u5141\u8bb8\u666e\u901a\u9ad8\u6821\u5728\u6821\u751f\uff08\u542b\u9ad8\u6821\u65b0\u751f\uff09\u5e94\u5f81\u5165\u4f0d\u670d\u4e49\u52a1\u5175\u5f79\u9000\u5f79\uff0c\u5728\u5b8c\u6210\u672c\u79d1\u5b66\u4e1a\u540e( )\u5e74\u5185\u53c2\u52a0\u5168\u56fd\u7855\u58eb\u7814\u7a76\u751f\u62db\u751f\u8003\u8bd5\uff0c\u521d\u8bd5\u603b\u5206\u52a0(  )\u5206\uff0c\u540c\u7b49\u6761\u4ef6\u4e0b\u4f18\u5148\u5f55\u53d6\u3002'], [u'\u201c\u6c5f\u82cf\u7701\u9ad8\u6821\u6bd5\u4e1a\u751f\u5c31\u4e1a\u7f51\u7edc\u8054\u76df\u201d\u7684\u7f51\u5740\u662f\u4ec0\u4e48\uff1f'], [u'\u201c\u4e94\u9669\u4e00\u91d1\u201d\u91cc\u7684\u201c\u4e00\u91d1\u201d\u662f\u6307\uff1a'], [u'\u6211\u7701\u57f9\u517b\u519c\u6751\u5b66\u6821\u6559\u80b2\u7855\u58eb\u5e08\u8d44\u63a8\u8350\u4eba\u9009\uff0c\u6240\u5b66\u4e13\u4e1a\u4e0d\u5305\u62ec\uff1a'], [u'\u4e0b\u5217\u5173\u4e8e\u52b3\u52a8\u8005\u5728\u8bd5\u7528\u671f\u7684\u5de5\u8d44\u8bf4\u6cd5\u6b63\u786e\u7684\u662f\uff1a'], [u'\u76f4\u62db\u58eb\u5b98\u5165\u4f0d\u540e\u5982\u4f55\u53d1\u653e\u5de5\u8d44\uff1f'], [u'\u4ea4\u4e92\u5f71\u54cd\u751f\u547d\u5386\u7a0b\u7684\u4e09\u79cd\u65cb\u5f8b\u5305\u62ec\uff1a(1)\u5de5\u4f5c\u3001\u804c\u4e1a\u4e0e\u4e8b\u4e1a (2)\u60c5\u611f\u3001\u5a5a\u59fb\u4e0e\u5bb6\u5ead (3)\u4e2a\u4eba\u8eab\u5fc3\u53d1\u5c55\u4e0e\u81ea\u6211\u7684\u6210\u957f (4)\u5b66\u5386\u3001\u5b66\u8bc6\u4e0e\u6587\u5316\u4fee\u517b\u3002'], [u'\u5728\u6cd5\u5b9a\u52b3\u52a8\u5e74\u9f84\u5185\uff0c\u6709\u52b3\u52a8\u80fd\u529b\uff0c\u6709\u5c31\u4e1a\u8981\u6c42\uff0c\u5904\u4e8e\u65e0\u4e1a\u72b6\u6001\u7684\u57ce\u9547\u5e38\u4f4f\u4eba\u5458\uff0c\u5728\u516c\u5171\u5c31\u4e1a\u670d\u52a1\u673a\u6784\u8fdb\u884c\u5931\u4e1a\u767b\u8bb0\uff0c\u7533\u9886\u300a\u5c31\u4e1a\u5931\u4e1a\u767b\u8bb0\u8bc1\u300b\u3002\u5176\u4e2d\uff0c\u519c\u6751\u8fdb\u57ce\u52a1\u5de5\u4eba\u5458\u548c\u5176\u4ed6\u975e\u672c\u5730\u6237\u7c4d\u4eba\u5458\u5728\u5e38\u4f4f\u5730\u7a33\u5b9a\u5c31\u4e1a\u6ee1\uff08\uff09\u4e2a\u6708\u7684\uff0c\u5931\u4e1a\u540e\u53ef\u4ee5\u5728\u5e38\u4f4f\u5730\u767b\u8bb0\u3002'], [u'\u201c\u897f\u90e8\u8ba1\u5212\u201d\u5fd7\u613f\u8005\u5230\u670d\u52a1\u7701\u57f9\u8bad\u5730\u62a5\u5230\u5e76\u53c2\u52a0\u7531\u670d\u52a1\u7701\u9879\u76ee\u529e\u7edf\u4e00\u7ec4\u7ec7\u7684\u96c6\u4e2d\u57f9\u8bad\uff0c\u65f6\u95f4\u4e0d\u5c11\u4e8e\u51e0\u5929\uff1f'], [u'\u7532\u3001\u4e59\u3001\u4e19\u3001\u4e01\u6253\u7b97\u8bbe\u7acb\u4e00\u5bb6\u666e\u901a\u5408\u4f19\u4f01\u4e1a\u3002\u5bf9\u6b64\uff0c\u4e0b\u5217\u54ea\u4e00\u8868\u8ff0\u662f\u6b63\u786e\u7684\uff1f'], [u'\u5982\u679c\u7b80\u5386\u9488\u5bf9\u7684\u6c42\u804c\u76ee\u6807\u662f\u201c\u8ba1\u7b97\u673a\u7a0b\u5e8f\u5458\u201d\uff0c\u4ee5\u4e0b\u54ea\u4e2a\u7ecf\u5386z\u6700\u5e94\u91cd\u70b9\u7a81\u51fa\uff1a'], [u'\u300a\u6bd5\u4e1a\u751f\u53cc\u5411\u9009\u62e9\u5c31\u4e1a\u63a8\u8350\u8868\u300b\u4e0d\u614e\u9057\u5931\uff0c\u5e94\u5411\u4e0b\u5217\u54ea\u4e2a\u90e8\u95e8\u7533\u8bf7\u8865\u529e\uff1f'], [u'\u201c\u82cf\u5317\u8ba1\u5212\u201d\u4e2d\uff0c\u9664\u56e0\u4e0d\u53ef\u6297\u529b\u6216\u7279\u6b8a\u539f\u56e0\uff0c\u5fd7\u613f\u8005\u64c5\u79bb\u670d\u52a1\u5c97\u4f4d\u8fbe\u51e0\u65e5\u6216\u64c5\u81ea\u89e3\u9664\u534f\u8bae\u7684\uff0c\u89c6\u4e3a\u8fdd\u7ea6\u3002'], [u'\u6c5f\u82cf\u7701\u9ad8\u6821\u62db\u751f\u5c31\u4e1a\u6307\u5bfc\u670d\u52a1\u4e2d\u5fc3\u201c\u4e13\u5bb6\u5728\u7ebf\u201d\u54a8\u8be2\u5e73\u53f0\u662f\uff1a'], [u'\u4ee5\u4e0b\u9762\u8bd5\u7c7b\u578b\uff0c\u54ea\u4e00\u9879\u8ddf\u5176\u4ed6\u9879\u4e0d\u662f\u4e00\u7c7b\uff1f'], [u'\u4f60\u8ba4\u4e3a\u5728\u9762\u8bd5\u65f6\u4e0b\u5217\u8a00\u8c08\u4e3e\u6b62\u4e2d\u4e0d\u5408\u9002\u7684\u662f\uff1a'], [u'\u516c\u76ca\u6027\u5c97\u4f4d\u793e\u4f1a\u4fdd\u9669\u8865\u8d34\u671f\u9650\uff0c\u4e00\u822c\u6700\u957f\u4e0d\u8d85\u8fc7\u51e0\u5e74\uff1f'], [u'\u4e0b\u5217\u5bf9\u4e8e\u7ed3\u6784\u5316\u9762\u8bd5\u63cf\u8ff0\u4e0d\u6b63\u786e\u7684\u662f\uff1a'], [u'2016\u5e74\u5168\u56fd\u8ba1\u5212\u62db\u8058\u7279\u5c97\u6559\u5e08\u7ea6\uff08\uff09\u540d\u3002'], [u'\u9ad8\u6821\u6bd5\u4e1a\u751f\u5e94\u5f81\u5165\u4f0d\u540e\uff0c\u62a5\u8003\u519b\u6821\u5e74\u9f84\u9650\u5236\u4e3a\u4e0d\u8d85\u8fc7\uff08\uff09\u5468\u5c81\u3002'], [u'\u6784\u6210\u804c\u4e1a\u7684\u8981\u7d20\u6709\uff1a\uff081\uff09\u804c\u4e1a\u540d\u79f0\uff082\uff09\u804c\u4e1a\u4e3b\u4f53\uff083\uff09\u804c\u4e1a\u5ba2\u4f53\uff084\uff09\u804c\u4e1a\u62a5\u916c\uff085\uff09\u804c\u4e1a\u6280\u672f\uff086\uff09\u804c\u4e1a\u7d20\u517b\u3002'], [u'\u4ee5\u4e0b\u5173\u4e8e\u4e2a\u4eba\u72ec\u8d44\u4f01\u4e1a\u8868\u8ff0\u9519\u8bef\u7684\u662f\u54ea\u4e00\u9879\uff1f'], [u'\u56fd\u5bb6\u89c4\u5b9a\u5bf9\u7b26\u5408\u6761\u4ef6\u7684\u9ad8\u6821\u6bd5\u4e1a\u751f\u81ea\u4e3b\u521b\u4e1a\u7684\uff0c\u53ef\u5728\u521b\u4e1a\u5730\u6309\u89c4\u5b9a\u7533\u8bf7\u5c0f\u989d\u62c5\u4fdd\u8d37\u6b3e\uff1b\u4ece\u4e8b\u5fae\u5229\u9879\u76ee\u7684\uff0c\u53ef\u4eab\u53d7\u4e0d\u8d85\u8fc7\u591a\u5c11\u4e07\u5143\u8d37\u6b3e\u989d\u5ea6\u7684\u8d22\u653f\u8d34\u606f\u6276\u6301\uff1f'], [u'\u5750\u59ff\u793c\u4eea\u4e2d\u4e00\u822c\u5750\u6ee1\u6905\u5b50\u7684\uff08   \uff09\u3002'], [u'\u5bf9\u7b26\u5408\u6761\u4ef6\u7684\u57f9\u8bad\u673a\u6784\u57f9\u8bad\u7684\u4ece\u4e8b\u670d\u52a1\u5916\u5305\u4e1a\u52a1\u4eba\u624d\uff08\u5927\u4e13\u4ee5\u4e0a\u5b66\u5386\uff09\uff0c\u901a\u8fc7\u670d\u52a1\u5916\u5305\u4e13\u4e1a\u77e5\u8bc6\u548c\u6280\u80fd\u57f9\u8bad\u8003\u6838\uff0c\u5e76\u4e0e\u670d\u52a1\u5916\u5305\u4f01\u4e1a\u7b7e\u8ba21\u5e74\u671f\u4ee5\u4e0a\u52b3\u52a8\u5408\u540c\u7684\uff0c\u7ed9\u4e88\u57f9\u8bad\u673a\u6784\u6bcf\u4eba\u4e0d\u8d85\u8fc7\u591a\u5c11\u5143\u7684\u57f9\u8bad\u652f\u6301\u3002'], [u'\u201c\u4e09\u652f\u4e00\u6276\u201d\u4e2d\u201c\u4e09\u652f\u201d\u662f\u6307\uff1a'], [u'\u4e0b\u5217\u54ea\u4e9b\u60c5\u51b5\uff0c\u7528\u4eba\u5355\u4f4d\u4e0d\u5f97\u4e0e\u52b3\u52a8\u8005\u7ea6\u5b9a\u7531\u52b3\u52a8\u8005\u627f\u62c5\u8fdd\u7ea6\u91d1\uff1a'], [u'\u6c42\u804c\u81ea\u8350\u6750\u6599\u5e94\u9075\u5faa\u4ee5\u4e0b\u54ea\u4e9b\u539f\u5219\uff1a'], [u'\u201c\u9752\u5e74\u83dc\u541b\u201d\u662f\u65b9\u4fbf\u4e0a\u73ed\u5e74\u8f7b\u4eba\u505a\u665a\u996d\u7684\u51c0\u83dc\u9500\u552e\u516c\u53f8\uff0c\u8fd9\u4e2a\u521b\u4e1a\u673a\u4f1a\u662f\uff1a'], [u'\u4e0b\u5217\u54ea\u4e9b\u662f\u901a\u5e38\u8ba4\u4e3a\u7684"\u670d\u52a1\u5916\u5305"\u7684\u5206\u7c7b\uff1f'], [u'\u7b7e\u5b9a\u5c31\u4e1a\u534f\u8bae\u540e\uff0c\u5c1a\u672a\u6d3e\u9063\u7684\uff0c\u4e0b\u5217\u54ea\u79cd\u60c5\u51b5\u53ef\u4ee5\u4e0d\u89c6\u4e3a\u8fdd\u7ea6\uff1f'], [u'\u57f9\u517b\u670d\u52a1\u5916\u5305\u4eba\u624d\u7684\u91cd\u8981\u610f\u4e49\u5728\u4e8e\uff1a'], [u'\u9ad8\u6821\u6bd5\u4e1a\u751f\u5e94\u5f81\u5165\u4f0d\u670d\u4e49\u52a1\u5175\u5f79\u4eab\u53d7\u54ea\u4e9b\u4f18\u60e0\u653f\u7b56\uff1f'], [u'\u4ee5\u4e0b\u54ea\u4e9b\u505a\u6cd5\u5bb9\u6613\u5f15\u8d77\u9762\u8bd5\u5b98\u7684\u53cd\u611f\uff1a'], [u'\u6bd5\u4e1a\u751f\u5c31\u4e1a\u62e9\u4e1a\u5e38\u89c1\u7684\u51e0\u79cd\u5fc3\u7406\u8bef\u533a\uff1a'], [u'\u5de5\u4f24\u4fdd\u9669\u8d39\u7531\u7528\u4eba\u5355\u4f4d\u7f34\u7eb3\u3002'], [u'\u7533\u8bf7\u529e\u7406\u6539\u6d3e\u4ee5\u540e\uff0c\u539f\u300a\u5c31\u4e1a\u62a5\u5230\u8bc1\u300b\u5c31\u6ca1\u6709\u7528\u4e86\uff0c\u53ef\u4ee5\u4ea4\u7ed9\u539f\u63a5\u6536\u5355\u4f4d\u6216\u8005\u81ea\u884c\u4fdd\u7ba1\u3002'], [u'\u201c\u4e09\u652f\u4e00\u6276\u201d\u62db\u52df\u7684\u5927\u5b66\u751f\u662f\u5426\u9700\u8981\u8fdb\u884c\u4e0a\u5c97\u524d\u57f9\u8bad\uff1f'], [u'2016\u5e74\u6c5f\u82cf\u7701\u9009\u8058\u9ad8\u6821\u6bd5\u4e1a\u751f\u5230\u6751\u4efb\u804c\u7684\u5bf9\u8c61\u662f30\u5c81\u4ee5\u4e0b\u5e94\u5c4a\u548c\u5f80\u5c4a\u5168\u65e5\u5236\u666e\u901a\u9ad8\u6821\u4e13\u79d1\u53ca\u4ee5\u4e0a\u5b66\u5386\u6bd5\u4e1a\u751f\u3002'], [u'\u5728\u5b66\u4fe1\u7f51\u4e0a\u53ef\u4ee5\u9a8c\u8bc1\u6bd5\u4e1a\u8bc1\u4e66\u7684\u771f\u4f2a\u4fe1\u606f\uff0c\u5982\u679c\u67e5\u8be2\u4e0d\u5230\uff0c\u53ef\u4ee5\u5230\u6c5f\u82cf\u7701\u9ad8\u6821\u62db\u751f\u5c31\u4e1a\u6307\u5bfc\u670d\u52a1\u4e2d\u5fc3\u7533\u8bf7\u5b66\u5386\u8ba4\u8bc1\u3002'], [u'\u5de5\u4f5c\u65e5\u5fd7\u6cd5\u662f\u5c97\u4f4d\u5206\u6790\u7684\u4e00\u79cd\u65b9\u6cd5\u3002\u662f\u8ba9\u5458\u5de5\u4ee5\u5de5\u4f5c\u65e5\u8bb0\u6216\u5de5\u4f5c\u7b14\u8bb0\u7684\u5f62\u5f0f\u8bb0\u5f55\u65e5\u5e38\u5de5\u4f5c\u6d3b\u52a8\u800c\u83b7\u5f97\u6709\u5173\u5c97\u4f4d\u5de5\u4f5c\u4fe1\u606f\u8d44\u6599\u7684\u65b9\u6cd5\u3002'], [u'\u7528\u4eba\u5355\u4f4d\u8fdd\u53cd\u672c\u6cd5\u89c4\u5b9a\u89e3\u9664\u6216\u8005\u7ec8\u6b62\u52b3\u52a8\u5408\u540c\uff0c\u52b3\u52a8\u8005\u8981\u6c42\u7ee7\u7eed\u5c65\u884c\u52b3\u52a8\u5408\u540c\u7684\uff0c\u7528\u4eba\u5355\u4f4d\u5e94\u5f53\u7ee7\u7eed\u5c65\u884c\u3002'], [u'\u6709\u5173\u897f\u90e8\u8ba1\u5212\uff0c\u53ef\u767b\u9646\u4e2d\u56fd\u9752\u5e74\u7f51\u67e5\u8be2\u3002'], [u'\u6bd5\u4e1a\u751f\u4e2a\u4eba\u6863\u6848\u662f\u5f88\u91cd\u8981\u7684\u6750\u6599\uff0c\u5b83\u8bb0\u8f7d\u7740\u6bd5\u4e1a\u751f\u4ece\u4e2d\u5b66\u5230\u5927\u5b66\u7684\u5b66\u4e60\u6210\u7ee9\u3001\u653f\u6cbb\u8868\u73b0\u3001\u5956\u60e9\u7b49\u5185\u5bb9\uff0c\u662f\u4e2a\u4eba\u6210\u957f\u7684\u5386\u53f2\u89c1\u8bc1\u3002\u5bf9\u6bd5\u4e1a\u751f\u8eab\u4efd\u9274\u5b9a\u3001\u51fa\u56fd\u7559\u5b66\u3001\u7ee7\u7eed\u6df1\u9020\u3001\u804c\u4f4d\u5347\u8fc1\u3001\u4e2a\u4eba\u53d1\u5c55\u7b49\u65b9\u9762\u6709\u7740\u91cd\u8981\u7684\u4f5c\u7528\u3002'], [u'\u751f\u80b2\u4fdd\u9669\u8d39\u7531\u7528\u4eba\u5355\u4f4d\u7f34\u7eb3\u3002']]